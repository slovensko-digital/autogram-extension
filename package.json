{
  "name": "sk-sk-extension",
  "version": "1.0.3",
  "description": "Rozsirenie pre slovensko.sk, s lepsim podpisovacom",
  "main": "./webpack/manifest.ts",
  "scripts": {
    "start": "npm run webpack:build-dev && run-p webpack:watch webext:run",
    "start:manifest2": "SKSE_MANIFEST_VERSION=2 npm start",
    "start:manifest3": "SKSE_MANIFEST_VERSION=3 npm start",
    "lint": "eslint src webpack",
    "build": "run-s webpack:build-dev webext:build",
    "build:prod": "run-s webpack:build-prod webext:build",
    "webpack:watch": "webpack watch --config webpack/webpack.dev.ts",
    "webpack:build-prod": "NODE_ENV=production webpack --config webpack/webpack.prod.ts",
    "webpack:build-dev": "webpack --config webpack/webpack.dev.ts",
    "test": "npx jest",
    "webext:build": "web-ext build -s 'dist' -i '*.map.js' --overwrite-dest --filename $npm_package_name-$npm_package_version.zip",
    "webext:run": "web-ext build -s 'dist' --overwrite-dest "
  },
  "author": "pom",
  "license": "EUPL-1.2",
  "devDependencies": {
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/node": "^15.0.1",
    "@types/webpack": "^4.41.27",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-manifest-plugin": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^8.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-webpack-plugin": "^2.5.4",
    "fork-ts-checker-webpack-plugin": "^6.2.5",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^9.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "web-ext": "^6.1.0",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^3.1.1"
  },
  "dependencies": {
    "@octosign/client": "file:../white-label-client",
    "@types/chrome": "^0.0.145",
    "@types/firefox-webext-browser": "^82.0.0"
  }
}
