{
  "name": "autogram-extension",
  "version": "3.0.0",
  "description": "Rozšírenie umožnuje používať podpisovač Autogram od Slovensko.Digital na štátnych portáloch.",
  "type": "module",
  "main": "./webpack/manifest.ts",
  "scripts": {
    "start": "npm run webpack:build-dev && run-p webpack:watch webext:run",
    "start:manifest2": "AE_MANIFEST_VERSION=2 npm start",
    "start:manifest3": "AE_MANIFEST_VERSION=3 npm start",
    "lint": "eslint src webpack",
    "release": "./scripts/release.sh",
    "build:dev": "run-s webpack:build-dev webext:build",
    "build:dev:mv2": "AE_MANIFEST_VERSION=2 run-s webpack:build-dev webext:build",
    "build:prod": "run-s build:prod:mv2 build:prod:mv3",
    "build:prod:mv2": "AE_MANIFEST_VERSION=2 run-s webpack:build-prod webext:build",
    "build:prod:mv3": "AE_MANIFEST_VERSION=3 run-s webpack:build-prod webext:build",
    "build:prod:safari": "AE_MANIFEST_VERSION=2 run-s webpack:build-prod webext:build",
    "webpack:watch": "webpack watch --config webpack/webpack.dev.ts",
    "webpack:build-prod": "NODE_ENV=production webpack --config webpack/webpack.prod.ts",
    "webpack:build-dev": "webpack --config webpack/webpack.dev.ts",
    "manifest:webpack:watch-dev": "chokidar \"**/*.ts\" -c \"npm run webpack:build-dev\"",
    "test": "npx jest",
    "clean": "rm -r dist/* web-ext-artifacts/*",
    "webext:build": "web-ext build -s 'dist' -i '*.map.js' --overwrite-dest --filename $npm_package_name-mv$AE_MANIFEST_VERSION-$npm_package_version.zip",
    "webext:run": "web-ext build -s 'dist' --overwrite-dest ",
    "xcode-generate-safari-web-extension": "/Applications/Xcode.app/Contents/Developer/usr/bin/safari-web-extension-converter --macos-only --app-name autogram-extension --bundle-identifier digital.slovensko.autogram.extension --project-location ./safari/ ./dist/",
    "example-usage": "cd example-usage && vite --port 49675",
    "typecheck": "tsc --noEmit"
  },
  "author": "pom",
  "license": "EUPL-1.2",
  "devDependencies": {
    "@auto-it/conventional-commits": "^11.1.6",
    "@auto-it/git-tag": "^11.1.6",
    "@auto-it/upload-assets": "^11.1.6",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/jest": "^27.4.1",
    "@types/jscodeshift": "^0.11.11",
    "@types/node": "^20.17.12",
    "@types/webpack": "^4.41.40",
    "@types/webpack-dev-server": "^3.11.3",
    "@types/webpack-manifest-plugin": "^3.0.4",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "auto": "^11.1.6",
    "chokidar-cli": "^3.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^6.11.0",
    "eslint": "^8.57.0",
    "eslint-webpack-plugin": "^4.1.0",
    "fake-indexeddb": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^6.7.4",
    "prettier": "^3.2.5",
    "raw-loader": "^4.0.2",
    "serve": "^14.1.2",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "vite": "^5.2.11",
    "web-ext": "^7.4.0",
    "webpack": "^5.97.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.15.2",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "dependencies": {
    "@sentry/browser": "^8.48.0",
    "@types/chrome": "^0.0.145",
    "@types/css-font-loading-module": "^0.0.13",
    "@types/firefox-webext-browser": "^82.0.0",
    "@types/webextension-polyfill": "^0.8.2",
    "autogram-sdk": "^0.0.5",
    "crypto-browserify": "^3.12.0",
    "idb-keyval": "^6.2.1",
    "isomorphic-webcrypto": "^2.3.8",
    "jest-environment-jsdom": "^29.7.0",
    "js-base64": "^3.7.2",
    "loglevel": "^1.9.2",
    "loglevel-plugin-prefix": "^0.8.4",
    "stream-browserify": "^3.0.0",
    "vm-browserify": "^1.1.2",
    "webextension-polyfill": "^0.8.0",
    "zod": "^3.23.3"
  }
}
