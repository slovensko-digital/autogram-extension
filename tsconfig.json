{
  "compilerOptions": {
    "jsx": "react",
    "outDir": "./dist/",
    "moduleResolution": "node",
    // "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "module": "CommonJS",
    "target": "ES5",
    "allowJs": true,
    "lib": ["ES5", "ES2019", "DOM"],
    "resolveJsonModule": true
  },
  "exclude": ["spec"]
}
